                                <telerik:RadListBox.ItemsPanel>
                                    <ItemsPanelTemplate>
                                        <WrapPanel Orientation="Vertical" IsItemsHost="True"/>
                                    </ItemsPanelTemplate>
                                </telerik:RadListBox.ItemsPanel>
                                <i:Interaction.Triggers>
                                    <i:EventTrigger EventName="PreviewMouseDoubleClick">
                                        <i:InvokeCommandAction Command="{Binding CommandShowWindow}" CommandParameter="Top"></i:InvokeCommandAction>
                                    </i:EventTrigger>
                                    <i:EventTrigger EventName="GotFocus">
                                        <i:InvokeCommandAction Command="{Binding CommandCapTopPause}"></i:InvokeCommandAction>
                                    </i:EventTrigger>
                                    <i:EventTrigger EventName="LostFocus">
                                        <i:InvokeCommandAction Command="{Binding CommandCapTopReCover}"></i:InvokeCommandAction>
                                    </i:EventTrigger>
                                </i:Interaction.Triggers>
                            </telerik:RadListBox>
                            <DockPanel Margin="0,0,0,-2" Background="#FF021019" HorizontalAlignment="Stretch">
                                <Border BorderBrush="#FF66919A" BorderThickness="2,2,2,0">
                                    <TextBlock Text="目标预警" Foreground="White" FontSize="17" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                </Border>
                            </DockPanel>
                        </Grid>
                    </Border>
                    <GridSplitter Grid.ColumnSpan="2" VerticalAlignment="Bottom" HorizontalAlignment="Stretch" Height="34" Margin="0,0,0,-34" Background="Transparent"/>
                    <Grid Grid.Row="1" Grid.ColumnSpan="2" Margin="0,34,34,0">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="40"/>
                            <RowDefinition Height="auto"/>
                        </Grid.RowDefinitions>



                        <StackPanel Height="40" HorizontalAlignment="Right">
                            <CheckBox Content="预警弹出窗体" Height="30" Padding="5,0,0,0" Width="120" FontSize="16" Foreground="White" IsChecked="True" HorizontalAlignment="Right" VerticalContentAlignment="Center"/>
                        </StackPanel>
                        <telerik:RadListBox x:Name="capFaceLogWithImgListBox"  Grid.Row="1" BorderThickness="2" BorderBrush="#FF66919A" ItemsSource="{Binding CapList}" SelectedItem="{Binding CapTopListSelectedItem}" 
                          Background="Transparent" ItemTemplate="{StaticResource CaptureDataTemplate}" ItemContainerStyle="{StaticResource RadListBoxItemStyle}">
                            <telerik:RadListBox.ItemsPanel>
                                <ItemsPanelTemplate>
                                    <WrapPanel Orientation="Horizontal" IsItemsHost="True"/>
                                </ItemsPanelTemplate>
                            </telerik:RadListBox.ItemsPanel>
                            <i:Interaction.Triggers>
                                <i:EventTrigger EventName="PreviewMouseDoubleClick">
                                    <i:InvokeCommandAction Command="{Binding CommandEditFot}" CommandParameter="Top"></i:InvokeCommandAction>
                                </i:EventTrigger>
                                <i:EventTrigger EventName="GotFocus">
                                    <i:InvokeCommandAction Command="{Binding CommandCapTopPause}"></i:InvokeCommandAction>
                                </i:EventTrigger>
                                <i:EventTrigger EventName="LostFocus">
