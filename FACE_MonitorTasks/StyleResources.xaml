                    <telerik:RadMenuItem FontSize="13" Header="更多" HorizontalAlignment="Center" Height="Auto" VerticalAlignment="Center" Margin="5,0">

                        <telerik:RadMenuItem Header="编辑"  Command="{Binding CommandEditStrategy}" CommandParameter="{Binding Tag, ElementName=textBlock}" >
                            <telerik:RadMenuItem.Icon>
                                <Image Source="Images/Editor.png" />
                            </telerik:RadMenuItem.Icon>
                        </telerik:RadMenuItem>
                        <telerik:RadMenuItem Header="删除" Command="{Binding CommandDeleteStrategy}" CommandParameter="{Binding Tag, ElementName=textBlock}" >
