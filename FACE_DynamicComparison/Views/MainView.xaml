                                            <chartView:SplineSeries ItemsSource="{Binding Persons}" Stroke="#FF8EC441" PointTemplate="{StaticResource PointTemplate1}" CombineMode="None" ShowLabels="False" ValueBinding="Count" CategoryBinding="Time"/>
                                            <chartView:SplineSeries ItemsSource="{Binding Alerts}" Stroke="#FF1B9DDE" CategoryBinding="Time"  ShowLabels="False" ValueBinding="Count" PointTemplate="{StaticResource PointTemplate2}"/>
                                            <chartView:SplineSeries ItemsSource="{Binding Alarms}" Stroke="Red" CategoryBinding="Time"  ShowLabels="False" ValueBinding="Count" PointTemplate="{StaticResource PointTemplate3}"/>


                                        </chart:RadCartesianChart.Series>

                                    </chart:RadCartesianChart>

                                </Grid>
                                <Grid Grid.Row="2" Background="Transparent">
