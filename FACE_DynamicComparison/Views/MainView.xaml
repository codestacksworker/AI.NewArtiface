                                    <TextBlock Grid.Row="0" Grid.Column="1" Text="今日推送" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="12"/>
                                    <TextBlock Grid.Column="1" Grid.Row="1" Text="{Binding PushedCount,StringFormat={}{0:D2}}" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="12"/>

                                    <TextBlock Grid.Row="0" Grid.Column="2" Text="今日告警" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="12"/>
                                    <TextBlock Grid.Column="2" Grid.Row="1" Text="{Binding AlertedCount,StringFormat={}{0:D2}}" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="12"/>

                                    <TextBlock Grid.Row="0" Grid.Column="3" Text="今日人次" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="12"/>
                                    <TextBlock Grid.Column="3" Grid.Row="1" Text="{Binding PersonCount,StringFormat={}{0:D2}}" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="12"/>
                                </Grid>
                            </DockPanel>
                            <telerik:RadListBox x:Name="cmpFaceLogWithImgListBox" Grid.Row="1" Grid.ColumnSpan="2" BorderThickness="2" BorderBrush="#FF66919A" ItemsSource="{Binding CapCmpList}" SelectedItem="{Binding CurrentItem,Mode=TwoWay}" 
