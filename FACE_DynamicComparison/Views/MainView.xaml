                                        <BulletDecorator.Bullet>
                                            <Label Content="布控计划:" FontSize="14"/>
                                        </BulletDecorator.Bullet>
                                        <Label Content="永久布控/每天0:00~24:00" FontSize="14"/>
                                    </BulletDecorator>
                                    <BulletDecorator Grid.Row="2" SnapsToDevicePixels="True" FlowDirection="LeftToRight">
                                        <BulletDecorator.Bullet>
                                            <Label Content="布控人:" FontSize="14"/>
                                        </BulletDecorator.Bullet>
                                        <Label Content="XXXX" FontSize="14"/>
                                    </BulletDecorator>
                                    <BulletDecorator Grid.Row="3" SnapsToDevicePixels="True" FlowDirection="LeftToRight">
                                        <BulletDecorator.Bullet>
                                            <Label Content="描述:" FontSize="14"/>
                                        </BulletDecorator.Bullet>
                                        <Label Content="-------" FontSize="14"/>
                                    </BulletDecorator>

                                    <BulletDecorator Grid.Row="5" SnapsToDevicePixels="True" FlowDirection="LeftToRight">
                                        <BulletDecorator.Bullet>
                                            <Label Content="布控区域:" FontSize="14"/>
                                        </BulletDecorator.Bullet>
                                        <StackPanel Orientation="Horizontal">
                                            <Label Content="3个区域," FontSize="14"/>
                                            <Label Content="15个设备" FontSize="14"/>
                                        </StackPanel>
                                    </BulletDecorator>
                                    <Label Grid.Row="6" Content="区域1，区域2，区域3，区域4，区域5" FontSize="14"/>
                                    <BulletDecorator Grid.Row="8" SnapsToDevicePixels="True" FlowDirection="LeftToRight">
                                        <BulletDecorator.Bullet>
                                            <Label Content="布控目标:" FontSize="14"/>
                                        </BulletDecorator.Bullet>
                                        <StackPanel Orientation="Horizontal">
                                            <Label Content="4个模板," FontSize="14"/>
                                            <Label Content="11,301个目标" FontSize="14"/>
