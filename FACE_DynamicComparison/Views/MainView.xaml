                                    </BulletDecorator>
                                    <Label Grid.Row="9" Content="目标库1，目标库2，目标库3，目标库4" FontSize="14"/>
                                    <BulletDecorator Grid.Row="11" SnapsToDevicePixels="True" FlowDirection="LeftToRight">
                                        <BulletDecorator.Bullet>
                                            <Label Content="比对策略:" FontSize="14"/>
                                        </BulletDecorator.Bullet>
                                        <StackPanel Orientation="Horizontal">
                                            <Label Content="常规策略" FontSize="14"/>
