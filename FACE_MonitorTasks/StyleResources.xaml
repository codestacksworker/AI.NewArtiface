                        </Grid>
                        <glwttc:FieldFilterControl x:Name="PART_FieldFilterControl"  Grid.Row="1" Visibility="Collapsed" obt:StyleManager.Theme="{StaticResource Theme}" />


                        <!--<Border Padding="3" Width="20" Height="15" BorderThickness="0" VerticalAlignment="Center">
                            <Path Stretch="Fill" Fill="White" Data="F1 M 0,0L 20,0L 10,20L 0,0 Z " Stroke="Gray" StrokeThickness="1"/>
