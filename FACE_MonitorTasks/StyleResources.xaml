    <DataTemplate x:Key="taskDetailTemplate">
        <StackPanel Orientation="Horizontal">
            <TextBlock x:Name="textBlock" Tag="{Binding Uuid}" Visibility="Collapsed"/>
            <telerik:RadButton DataContext="{Binding DataContext, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type local:MainView}}}"  Command="{Binding CommandDetailStrategy}" CommandParameter="{Binding Tag, ElementName=textBlock}" BorderThickness="0" Background="Transparent" Content="查看"  HorizontalAlignment="Center" VerticalAlignment="Center"  Margin="10,0,10,0"/>

        </StackPanel>

    </DataTemplate>
    <DataTemplate x:Key="editMethodTemplate">
        <ContentControl Foreground="{Binding ElementName=txtScore,Path=Foreground}" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" d:DesignWidth="800" d:DesignHeight="100">
            <StackPanel x:Name="spMethod"  Margin="0,8"  HorizontalAlignment="Stretch" Tag="{Binding DataContext, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type local:EditStrategyForm}}}" VerticalAlignment="Center">
                <Border Visibility="{Binding MethodType, Converter={StaticResource IntToVisibilityReverseConverter}}" Margin="0">
                    <Grid HorizontalAlignment="Stretch">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="Auto"/>
                        </Grid.ColumnDefinitions>
                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Stretch" d:LayoutOverrides="Width" VerticalAlignment="Center">
                            <TextBlock Text="{Binding Index}" Margin="0" VerticalAlignment="Center"/>
                            <TextBlock Text="方法:  阈值法"  Margin="8,0" VerticalAlignment="Center"/>
                            <!--<telerik:RadComboBox Width="100"  DataContext="{Binding ElementName=spMethod,Path=Tag}" SelectedItem="{Binding SelectedMethodType,Mode=TwoWay}" ItemsSource="{Binding MethodTypeList}" DisplayMemberPath="TypeName" SelectedIndex="0" Margin="3,0,15,0"/>-->

                            <TextBlock Text="相似度 ≥: " VerticalAlignment="Center"/>
                            <TextBox x:Name="txtScore" Validation.ErrorTemplate="{StaticResource errorTemplateSilverlightStyle}" Text="{Binding ThresholdScore, Mode=TwoWay}" Width="80" VerticalAlignment="Center" Margin="3,0"/>

                        </StackPanel>
                        <telerik:RadButton Background="{x:Null}" BorderBrush="{x:Null}" CommandParameter="{Binding Uuid}"   Command="{Binding DataContext.CommandRemoveMethod, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type local:EditStrategyForm}}}" HorizontalAlignment="Right" Width="Auto" Height="Auto" Grid.Column="1" VerticalAlignment="Center" BorderThickness="0" Padding="4" ScrollViewer.CanContentScroll="True" Margin="0,0,5,0">
                            <Rectangle StrokeThickness="0" Fill="{StaticResource closeAction}" Width="14" Height="14"></Rectangle>
                        </telerik:RadButton>
                    </Grid>
                </Border>
                <Border Visibility="{Binding MethodType, Converter={StaticResource IntToVisibilityConverter}}">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="Auto"/>
                        </Grid.ColumnDefinitions>
                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" Visibility="{Binding MethodType, Converter={StaticResource IntToVisibilityConverter}}" Grid.ColumnSpan="2" VerticalAlignment="Center" Margin="0,3">
                            <TextBlock Text="{Binding Index}"  Margin="0" VerticalAlignment="Center"/>
                            <TextBlock Text="方法:  计数法"  Margin="8,0" VerticalAlignment="Center"/>
                            <!--<telerik:RadComboBox DataContext="{Binding ElementName=spMethod,Path=Tag}" Width="100" SelectedItem="{Binding SelectedMethodType,Mode=TwoWay}"  ItemsSource="{Binding MethodTypeList}" DisplayMemberPath="TypeName" SelectedIndex="1" Margin="3,0,15,0"/>-->

                            <TextBlock Text="在" VerticalAlignment="Center"/>
                            <TextBox Text="{Binding CalculateInterval, Mode=TwoWay}" Width="80" VerticalAlignment="Center" Margin="3,0"/>
                            <TextBlock Text="秒内抓拍的人脸  与  目标人相似度≥" VerticalAlignment="Center"/>
                            <TextBox Text="{Binding CalculateScore, Mode=TwoWay}" Width="80" VerticalAlignment="Center" Margin="3,0"/>
                            <TextBlock Text="，计数值次数≥" VerticalAlignment="Center"/>
                            <TextBox Text="{Binding CalculateTotal, Mode=TwoWay}" Width="80" VerticalAlignment="Center" Margin="3,0"/>

                        </StackPanel>
                        <telerik:RadButton Background="{x:Null}" BorderBrush="{x:Null}" CommandParameter="{Binding Uuid}"   Command="{Binding DataContext.CommandRemoveMethod, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type local:EditStrategyForm}}}" HorizontalAlignment="Right" Width="Auto" Height="Auto" Grid.Column="1" VerticalAlignment="Center" BorderThickness="0" Padding="4" ScrollViewer.CanContentScroll="True" Margin="0,0,5,0">
                            <Rectangle Opacity="0.7" StrokeThickness="0" Fill="{StaticResource closeAction}" Width="14" Height="14"></Rectangle>
                        </telerik:RadButton>
                    </Grid>
                </Border>
            </StackPanel>
        </ContentControl>
    </DataTemplate>
    
</ResourceDictionary>
