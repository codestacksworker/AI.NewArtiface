                        <DockPanel Margin="0,0,0,-2" Background="#FF021019" HorizontalAlignment="Left" Width="130">
                            <Border BorderBrush="#FF66919A" BorderThickness="2,2,2,0">
                                <TextBlock Text="拍摄预览" Foreground="White" HorizontalAlignment="Center" FontSize="17" VerticalAlignment="Center"/>
                            </Border>
                        </DockPanel>
                    </Grid>
                </Grid>
            </Border>
        </Grid>
